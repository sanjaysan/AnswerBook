{
  "name": "AnswerBook",
  "version": "0.0.1",
  "description": "Q & A forum for organizations",
  "main": "app.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "dev": "node app",
    "server": "nodemon app",
    "client": "npm start --prefix answerbookui",
    "heroku-postbuild": "cd answerbookui/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "cacheDirectories": [
    "node_modules",
    "answerbookui/node_modules"
  ],
  "dependencies": {
    "@babel/node": "^7.0.0-beta.37",
    "bcryptjs": "*",
    "body-parser": "*",
    "concurrently": "^3.5.1",
    "cors": "*",
    "express": "*",
    "jsonwebtoken": "*",
    "mongoose": "^5.0.0-rc0",
    "passport": "*",
    "passport-jwt": "^3.0.1",
    "pg": "^7.4.0",
    "pg-format": "^1.0.4",
    "sequelize": "^4.28.6"
  },
  "author": "Sanjay, Varun, Shivanee, Aravind, Palani, Sugadev",
  "license": "ISC"
}
